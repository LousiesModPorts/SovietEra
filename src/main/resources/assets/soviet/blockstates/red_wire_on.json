{
    "multipart": [
        {   "when": { "default": "true" },
            "apply": { "model": "soviet:red_wire_center" }
        },
     	{   "when": { "down": "true" },
            "apply": { "model": "soviet:red_wire_down", "uvlock": true }
        },
     	{   "when": { "up": "true" },
            "apply": { "model": "soviet:red_wire_up", "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "soviet:red_wire_north", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "soviet:red_wire_east", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "soviet:red_wire_south","uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "soviet:red_wire_west", "uvlock": true }
        }
    ]
}
