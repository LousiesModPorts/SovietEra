{
    "multipart": [
        {   "when": { "default": "true" },
            "apply": { "model": "soviet:vent_pipe_default" }
        },
     	{   "when": { "down": "true" },
            "apply": { "model": "soviet:vent_pipe_down", "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "soviet:vent_pipe_north", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "soviet:vent_pipe_east", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "soviet:vent_pipe_south","uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "soviet:vent_pipe_west", "uvlock": true }
        }
    ]
}
